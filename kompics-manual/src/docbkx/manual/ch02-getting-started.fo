			<fo:inline font-weight="bold">public</fo:inline> <fo:inline font-weight="bold">class</fo:inline> Root <fo:inline font-weight="bold">extends</fo:inline> ComponentDefinition {
			
				<fo:inline font-weight="bold">private</fo:inline> Component hostComponent;
				
