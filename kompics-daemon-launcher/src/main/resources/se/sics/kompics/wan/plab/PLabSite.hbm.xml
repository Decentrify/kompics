<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="se.sics.kompics.wan.plab">
	<class name="PLabSite" table="PLabSite">

		<id name="siteId" type="int" column="site_id">
			<generator class="native" />
		</id>

		<property name="name" type="string" column="name"/>

		<property name="abbreviatedName" column="abbreviated_name"/>
		
		<property name="loginBase" column="login_base"/>		
		
	</class>
</hibernate-mapping>
