			<index column="idx" />
			<one-to-many class="se.sics.kompics.plab.PLabHost" />
		</list>
		

		<list name="sites" cascade="all" column="sites">
			<key column="parent_id" />
			<index column="idx" />
			<one-to-many class="se.sics.kompics.plab.PLabSite" />
		</list>
 -->

		<list name="sliceNodes" cascade="all">
			<key column="parent_id" />
			<index column="idx" />
			<one-to-many class="java.lang.Integer" />
		</list>


	</class>
</hibernate-mapping>
