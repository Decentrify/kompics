<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
     "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="se.sics.kompics.wan.master.plab">
	<class name="PLabHost" table="PLabHost">

		<id name="node_id" type="java.lang.Integer" column="node_id">
			<generator class="native" />
		</id>

		<property name="hostname" type="java.lang.String" column="hostname" />

		<property name="boot_state" />
		
		<property name="bwlimit" />		
		
		<property name="ip" />
		
		<property name="site" />
		
		<property name="cpuLoad" />
		
	</class>
</hibernate-mapping>
