<?xml version="1.0" encoding="UTF-8"?><project>
  <parent>
    <artifactId>kompics-p2p</artifactId>
    <groupId>se.sics.kompics</groupId>
    <version>0.2.1-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>se.sics.kompics</groupId>
  <artifactId>p2p-infrastructure</artifactId>
  <name>Kompics P2P Infrastructure</name>
  <version>0.2.1-SNAPSHOT</version>
  <description>Kompics P2P Infrastructure</description>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <configuration>
          <archive>
            <manifest>
              <mainClass>se.sics.kompics.p2p.P2pLauncher</mainClass>
            </manifest>
          </archive>
          <descriptors>
            <descriptor>src/main/assembly/src.xml</descriptor>
          </descriptors>
          <finalName>kompics</finalName>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>se.sics.kompics</groupId>
      <artifactId>chord</artifactId>
      <version>0.2.1-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>se.sics.kompics</groupId>
      <artifactId>bootstrap</artifactId>
      <version>0.2.1-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>se.sics.kompics</groupId>
      <artifactId>failure-detector</artifactId>
      <version>0.2.1-SNAPSHOT</version>
    </dependency>
  </dependencies>
</project>